generator client {
  output   = env("PRISMA_GEN_RS_CLIENT_PATH")
  provider = "mise prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  pwd      String // hashed
  salt     String // hex encoded salt
  username String @unique
}
