generator client {
  output   = env("PRISMA_GEN_RS_CLIENT_REL_PATH")
  provider = "mise prisma"
}

datasource db {
  provider = "postgresql"
  // FIXME
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  email    String @unique
}
